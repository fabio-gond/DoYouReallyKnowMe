<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.android.doyoureallyknowme.SettingEndFragment">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Le tue risposte sono state salvate!" />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Ora tocca al tuo partner" />
    <Button
        android:id="@+id/btn_settingend_next"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/all_btnnext"/>

</LinearLayout>
