<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.android.doyoureallyknowme.QuizActivity"
    >

    <FrameLayout android:id="@+id/layout_quiz_quizcontainer"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="@dimen/paddingMain">
    </FrameLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="73dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent">
        <TextView
            android:id="@+id/textview_quiz_bottomText"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="1 di 10"
            android:gravity="center"/>

    </LinearLayout>


</android.support.constraint.ConstraintLayout>
